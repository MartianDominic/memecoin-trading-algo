{
  "name": "memecoin-trading-algo",
  "version": "1.0.0",
  "description": "Comprehensive memecoin trading algorithm with real-time data aggregation and analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "start:aggregator": "node dist/services/token-aggregator.service.js",
    "start:api": "node dist/api/api-server.js",
    "start:websocket": "node dist/api/websocket/websocket-manager.js",
    "deploy": "./scripts/deploy-complete-system.sh",
    "start:all": "./scripts/start-all.sh",
    "stop:all": "./scripts/stop-all.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov",
    "test:frontend": "jest --config=tests/frontend/jest.config.js",
    "test:backend": "jest --config=tests/backend/jest.config.js",
    "test:integration": "jest --config=tests/integration/jest.config.js",
    "test:e2e": "jest --config=tests/e2e/jest.config.js",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "psql -h $DB_HOST -p $DB_PORT -U $DB_USERNAME -d $DB_NAME -f src/database/migrations/001_initial_schema.sql",
    "db:seed": "node dist/database/seed.js",
    "health": "curl http://localhost:3000/health",
    "logs": "tail -f logs/app.log"
  },
  "keywords": [
    "memecoin",
    "trading",
    "algorithm",
    "solana",
    "dex",
    "real-time",
    "cryptocurrency",
    "trading-bot"
  ],
  "author": "Galaxy Developer",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "react-hook-form": "^7.63.0",
    "socket.io": "^4.7.4",
    "tailwind-merge": "^3.3.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "msw": "^2.3.5",
    "nodemon": "^3.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
