{
  "name": "memecoin-trading-algo",
  "version": "1.0.0",
  "description": "Advanced token detection pipeline with comprehensive testing",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest tests/unit --coverage",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:security": "npm audit && snyk test",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src tests --ext .js,.ts",
    "lint:fix": "eslint src tests --ext .js,.ts --fix",
    "build": "tsc",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "validate": "npm run lint && npm run test:ci"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "redis": "^4.6.0",
    "web3": "^4.2.0",
    "ethers": "^6.8.0",
    "ws": "^8.14.0",
    "node-cache": "^5.1.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "artillery": "^2.0.0",
    "snyk": "^1.1200.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "redis-memory-server": "^0.10.0",
    "mock-redis": "^0.56.3",
    "nock": "^13.4.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/**/*.d.ts",
      "!src/**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "testMatch": [
      "**/tests/**/*.test.{js,ts}",
      "**/tests/**/*.spec.{js,ts}"
    ],
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testTimeout": 30000
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "memecoin",
    "trading",
    "algorithm",
    "token-detection",
    "blockchain",
    "defi"
  ],
  "author": "Hive Mind Testing Agent",
  "license": "MIT"
}